#!/bin/bash


case "$1" in
*.tgz | *.tar.gz) tar tf "$1" ;;
*.tar.bz2 | *.tbz2) tar tjf "$1" ;;
*.tar.txz | *.txz) xz --list "$1" ;;
*.tar) tar tf "$1" ;;
*.zip | *.jar | *.war | *.ear | *.oxt) unzip -l "$1" ;;
*.rar) unrar l "$1" ;;
*.7z) 7z l "$1" ;;
*.[1-8]) man "$1" | col -b ;;
*.o) nm "$1" | less ;;
*.iso) iso-info --no-header -l "$1" ;;
*.odt | *.ods | *.odp | *.sxw) odt2txt "$1" ;;
*.pdf) pdftotext "$1" - ;;
*.docx) docx2txt "$1" - ;;
*.doc) antiword "$1" ;;
*.csv) cat "$1" | sed s/,/\\n/g ;;
*.wav | *.mp3 | *.flac | *.m4a | *.wma | *.ape | *.ac3 | *.og[agx] | *.spx | *.opus | *.as[fx] | *.jpg | *.png) exiftool "$1" ;;
*) cat "$1" ;;
esac
